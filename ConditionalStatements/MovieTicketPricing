package Guvi;
import java.util.Scanner;
public class MovieTicket {
 public static void main(String[] str) {
	 int age,price=0;
	 Scanner sc=new Scanner(System.in);
	 System.out.println("Enter Age: ");
	 age=sc.nextInt();
	 if(age<12) {
		 price=100;
		 System.out.println(price);
	 }else if(age>=12&&age<=59) {
		 price=200;
		 System.out.println(price);
	 }else {
		 price=150;
		 System.out.println(price);
	 }
 }
}
