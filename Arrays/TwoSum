package arrays;
import java.util.*;
public class TwoSum {
	public static void main(String[] str) {
		int[] arr= {2,3,4,1,7,6};
		int k=5;
		int count=0;
		for(int i=0;i<arr.length;i++) {
			for(int j=1;j<arr.length;j++) {
				if(arr[i]+arr[j]==k) {
					System.out.println(arr[i]+","+arr[j]);
					count++;
				}
			}
		}
		System.out.println("Total Count: "+count);
	}
}
