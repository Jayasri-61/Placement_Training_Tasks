package arrays;
import java.util.*;
public class CountEvenOdd {
	public static void main(String[] str) {
		int[] num= {2,4,6,7,8,9};
		int evenCount=0;
		int oddCount=0;
		for(int i=0;i<num.length;i++) {
			if(num[i]%2==0) {
				evenCount++;
			}else {
				oddCount++;
			}
		}
		System.out.println("Even Count: "+evenCount);
		System.out.println("Odd Count : "+oddCount);
	}
}
